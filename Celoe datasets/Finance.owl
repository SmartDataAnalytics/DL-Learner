        <rdfs:comment xml:lang="en">Political symptoms on which a risk profile is based.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.fadyart.com/Finance.owl#PrimaryMarket -->

    <owl:Class rdf:about="http://www.fadyart.com/Finance.owl#PrimaryMarket">
        <rdfs:label xml:lang="en">Primary markets</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.fadyart.com/Finance.owl#SecuritiesHandling"/>
        <rdfs:comment xml:lang="en">Entity in charge of handling the primary market orders.
