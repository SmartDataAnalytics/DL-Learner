                    <rdf:Description rdf:about="http://www.fadyart.com/Finance.owl#FixedAssets"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.fadyart.com/Finance.owl#BalanceSheet"/>
        <rdfs:comment xml:lang="en">The assets of the entity where-off the accounting is done.</rdfs:comment>
