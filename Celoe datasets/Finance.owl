    <!-- http://www.fadyart.com/Finance.owl#RiskProfile -->

    <owl:Class rdf:about="http://www.fadyart.com/Finance.owl#RiskProfile">
        <rdfs:label xml:lang="en">Risk profile</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.fadyart.com/Finance.owl#hasRiskSymptom"/>
