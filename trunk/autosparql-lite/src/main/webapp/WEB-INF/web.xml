<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
<display-name>AutoSPARQL-Lite</display-name>	

 <servlet>
  	<servlet-name>AutoSPARQLService</servlet-name>
  	<servlet-class>org.autosparql.server.AutoSPARQLServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AutoSPARQLService</servlet-name>
  	<url-pattern>/Application/AutoSPARQLService</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>Application.html</welcome-file>
  </welcome-file-list>

</web-app>
