    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TIER: pos</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TAG: ADV</rdfs:label>
    <olia_system:hasTag rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ADV</olia_system:hasTag>
    <rdf:type rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/stts.owl#Adverb"/>
