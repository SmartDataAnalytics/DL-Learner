    <rdfs:domain rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger-syntax.owl#SyntacticNode"/>
    <rdfs:subPropertyOf rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger-syntax.owl#hasFunction"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">function labels within an adjective phrase</rdfs:comment>
