    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The tag "$." is used for sentence final punctuation e.g. ". ? ! ; :".</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
