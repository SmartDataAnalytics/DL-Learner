    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">open word classes
(tigerstts.xml)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A71">
    <rdf:rest rdf:nodeID="A74"/>
    <rdf:first rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/stts.owl#ProperNoun"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger.owl#neut">
    <olia_system:hasTier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">morph</olia_system:hasTier>
    <j.0:hasGender rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger.owl#neut"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">neutral gender</rdfs:comment>
    <olia_system:hasTagContaining rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Neut</olia_system:hasTagContaining>
    <rdf:type rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger.owl#GenderFeature"/>
