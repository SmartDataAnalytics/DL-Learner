    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/stts.owl#CardinalNumber"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://nlp2rdf.org/ontology/nounPhrase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger-syntax.owl#NounPhrase"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://nachhalt.sfb632.uni-potsdam.de/owl/stts.owl#NNE">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/stts.owl#ProperNoun"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TAG: NNE</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TIER: pos</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is an additional tag used in the NEGRA Corpus (not in Schiller et al. 1999). It refers to common nouns that occur as part of a named entity. In a strict sense, these would have to be in the intersection between CommonNoun and ProperNoun, but as these are disjoint in the Reference Model, this leads to an inconsistency. Thus, in accordance with their function, NNEs are regarded as ProperNouns here, not as CommonNouns.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://nlp2rdf.org/ontology/chunk">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://nachhalt.sfb632.uni-potsdam.de/owl/tiger-syntax.owl#SyntacticNode"/>
  </rdf:Description>
